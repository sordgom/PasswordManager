{
  "manifest_version":2,
  "version":"1.0",
  "name":"Konache",

  "description": "This is a web extension that allows you to auto login to your favorite websites using Konache password manager",

  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["main.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "http://*/*",
    "https://*/*",
    "http://localhost:8080/*"
  ]
}